<div id="main-screen">

    <div id="menu-header">

        <img id="logoWMainie" src="./img/icons/{{background}}.svg">
    </div>

    <table id="main-menu-table" class="table-hover">
        <tr>
            <td class="info">
                <p id="one">
                    {{one}}
                    <img class="center-block img-responsive"
                         src="./img/icons/{{one}}.svg"
                         style="fill: '#5ecbea'"/>
                </p>

            </td>
            <td class="info"><p id="two">{{two}}<img class="center-block img-responsive"
                                                     src="./img/icons/{{two}}.svg">
            </p>
            </td>

        </tr>
        <tr>
            {{#ifCond three "brak"}}

            {{else}}
                <td class="info"><p id="{{three}}">{{three}}<img class="center-block img-responsive"
                                                                 src="./img/icons/{{three}}.svg">
                </p>
                </td>
            {{/ifCond}}

            {{#ifCond four "brak"}}

            {{else}}
                <td class="info"><p id="{{four}}">{{four}}<img class="center-block img-responsive"
                                                               src="./img/icons/{{four}}.svg">
                </p>
                </td>
            {{/ifCond}}
        </tr>
        <tr>
            {{#ifCond five "brak"}}

            {{else}}
                <td class="info"><p id="{{five}}">{{five}}<img class="center-block img-responsive"
                                                               src="./img/icons/{{five}}.svg">
                </p>
                </td>
            {{/ifCond}}

            {{#ifCond six "brak"}}

            {{else}}
                <td class="info"><p id="{{six}}">{{six}}<img class="center-block img-responsive"
                                                             src="./img/icons/{{six}}.svg">
                </p>
                </td>
            {{/ifCond}}
        </tr>
    </table>
</div>
